(executables
 (names main compiler dockerUtils lucidcc dfgCompiler eventParsers)
 (libraries dpt)
 (link_flags -cc g++)
 )

(install
 (section bin)
 (files 
    (main.exe as dpt) 
    (compiler.exe as compiler) 
    (dockerUtils.exe as dockerUtils)
    (lucidcc.exe as lucidcc)
    (dfgCompiler.exe as dfgCompiler) 
    (eventParsers.exe as eventParsers)
    )
 )