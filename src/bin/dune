 (files 
    (main.exe as dpt) 
    (compiler.exe as compiler) 
    (dockerUtils.exe as dockerUtils)
    (cCompiler.exe as cCompiler)
    (dfgCompiler.exe as dfgCompiler) 
    (eventParsers.exe as eventParsers)
    )
    )