(executables
 (names interpMain lucidp4t dockerUtils lucidcc dfgCompiler eventParsers lucidSwitch)
;  (names main compiler dockerUtils lucidcc dfgCompiler eventParsers)
 (libraries dpt)
 (link_flags -cc g++)
 )

(install
 (package dpt)
 (section bin)
 (files 
    (interp.exe as dpt) 
    (lucidSwitch.exe as lucidSwitch)
    (lucidp4t.exe as compiler) 
    (dockerUtils.exe as dockerUtils)
    (lucidcc.exe as lucidcc)
    (dfgCompiler.exe as dfgCompiler) 
    (eventParsers.exe as eventParsers)
    )
 )